<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.EatClean" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#00EEE6</item>
        <item name="colorPrimaryVariant">#1B3148</item>
        <item name="colorOnPrimary">#282118</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/btn</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="switchNoti" parent="Theme.AppCompat.Light">
        <item name="colorControlActivated">@color/btn</item>
        <item name="colorSwitchThumbNormal">@color/white</item>
    </style>

    <style name="Button.Primary" parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">@color/btn</item>
    </style>

    <style name="editText" >
        <item name="colorControlNormal">@color/fontText2</item>
        <item name="colorControlActivated">@color/btn</item>
        <item name="colorControlHighlight">#FFFFFF</item>
    </style>

</resources>